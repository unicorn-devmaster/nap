{
  "name": "nap",
  "version": "0.24.1",
  "main": "index.js",
  "scripts": {
    "setup": "echo \"# See .env.example for more\" > .env",
    "lint": "eslint . || true",
    "_dev": "next",
    "_prod": "NODE_ENV=production npm run _build && npm run _serve",
    "_build": "next build",
    "_start": "next start",
    "_serve": "NODE_ENV=production node index.js",
    "_serve-dev": "nodemon --inspect=0.0.0.0:5858 index.js",
    "_build-front": "next build && next start",
    "_build-back": "next build && npm run _serve",
    "_build-back-dev": "next build && npm run _serve-dev",
    "dev": "npm run exec -- npm run _dev",
    "build": "npm run exec -- npm run _build",
    "start": "npm run exec -- npm run _start",
    "serve": "npm run exec -- npm run _serve",
    "prod": "npm run exec -- npm run _prod",
    "local": "npm run stop && REDIS_URI=redis://localhost MONGODB_URI=mongodb://localhost/graphql npm run _serve",
    "image": "docker build --no-cache -t rabbotio/nap . || true",
    "image-dev": "NODE_ENV=development docker build --no-cache -t rabbotio/nap . || true",
    "history": "docker history rabbotio/nap > history.log && cat history.log || true",
    "size": "echo $(docker image inspect rabbotio/nap -f {{.Size}})",
    "in": "docker exec -it $(docker ps -a -q --filter ancestor=rabbotio/nap) /bin/sh || true",
    "exec": "docker exec -it $(docker ps -a -q --filter ancestor=rabbotio/nap)",
    "rmi": "docker rmi -f rabbotio/nap || true",
    "clean": "docker rm -f $(docker ps -a -q --filter ancestor=rabbotio/nap) || true",
    "run": "docker run -p 3000:3000 -d rabbotio/nap || true",
    "run-dev": "docker run -p 3000:3000 -p 5858:5858 -p 9229:9229 -d rabbotio/nap || true",
    "logs": "docker logs $(docker ps -a -q --filter ancestor=rabbotio/nap) || true",
    "stop": "docker stop $(docker ps -a -q --filter ancestor=rabbotio/nap) || true",
    "clear": "docker rm $(docker ps -aq) -f || true && docker rmi -f rabbotio/nap && docker system prune -f && docker images && docker ps -a || true",
    "down": "docker-compose kill || true && docker-compose down || true",
    "up": "docker-compose build && docker-compose up -d && docker ps || true",
    "up-dev": "NODE_ENV=development docker-compose -f docker-compose.yml -f docker-compose.dev.yml build && docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d && docker ps -a || true && npm run attach",
    "attach": "docker attach $(docker ps -a -q --filter ancestor=rabbotio/nap) || true",
    "graphiql": "docker run -it -p3001:3000 vitalcode/auth-graphiql",
    "test": "jest --watchAll --runInBand",
    "cover": "jest --coverage --no-cache"
  },
  "dependencies": {
    "apollo-client": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "connect-redis": "^3.3.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-bearer-token": "^2.1.0",
    "express-graphql": "^0.6.6",
    "express-session": "^1.15.3",
    "graphql": "^0.10.3",
    "graphql-compose": "1.21.1",
    "graphql-compose-connection": "^2.2.2",
    "graphql-compose-mongoose": "^1.5.1",
    "graphql-compose-recompose": "0.0.1",
    "graphql-compose-relay": "^1.0.15",
    "graphql-tag": "^2.4.2",
    "graphql-tools": "^1.1.0",
    "helmet": "^3.6.1",
    "is_js": "^0.9.0",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.4.1",
    "localforage": "^1.5.0",
    "lusca": "^1.4.1",
    "mailgun.js": "^2.0.1",
    "mongoose": "^4.11.1",
    "mongoose-role": "^2.0.1",
    "mubsub": "^1.4.0",
    "multer": "^1.3.0",
    "next": "2.4.6",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-facebook-token": "^3.3.0",
    "passport-github2": "^0.1.10",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "path-match": "^1.2.4",
    "platform": "^1.3.4",
    "promise-polyfill": "^6.0.2",
    "react": "^15.6.1",
    "react-apollo": "^1.4.3",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.1.0"
  },
  "author": "katopz@gmail.com",
  "license": "MIT",
  "repository": "https://github.com/rabbotio/nap",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.24.1",
    "caporal": "^0.7.0",
    "casual": "^1.5.14",
    "docker-build-run-push": "^3.0.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.1.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "nodemon": "^1.11.0",
    "promise-sequential": "^1.1.1",
    "prop-types": "^15.5.10",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1"
  }
}